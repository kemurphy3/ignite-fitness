{
  "name": "ignite-fitness",
  "version": "1.0.0",
  "description": "AI-powered fitness coaching Progressive Web App",
  "main": "js/app.js",
  "scripts": {
    "build": "webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "dev": "webpack serve --mode=development",
    "analyze": "webpack --mode=production --analyze",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "perf:budget": "node scripts/perf-budget.js",
    "perf:lighthouse": "lhci autorun",
    "perf:check": "npm run build && npm run perf:budget",
    "test:perf": "npm run perf:check",
    "test:smoke": "playwright test tests/smoke.spec.js",
    "test:imports": "node tools/check-imports.js",
    "test:syntax": "node tools/check-syntax.js",
    "test:dom": "node tools/check-dom-safety.js",
    "test:health": "node -e \"require('http').createServer((req, res) => { res.writeHead(200); res.end('OK'); }).listen(8001, () => console.log('Health check server running on :8001'));\"",
    "test:all": "npm run test:imports && npm run test:syntax && npm run test:dom && npm run test:smoke",
    "lint": "npm run test:syntax && npm run test:imports && npm run test:dom"
  },
  "dependencies": {
    "chart.js": "^4.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "cssnano": "^6.0.1",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.3.3",
    "rimraf": "^5.0.5",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.9.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "sideEffects": [
    "*.css",
    "*.scss",
    "*.sass",
    "*.less"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}